{
  "name": "hedgehog-api-test",
  "version": "3.0.3",
  "description": "Hedgehog API tests",
  "main": "handler.js",
  "scripts": {
    "analyse": "npm run lint && npm run test",
    "clean": "rimraf .build .serverless .nyc_output coverage",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "tslint --project . --format verbose",
    "pretest": "npm run clean",
    "test": "mocha --config ./src/.mocharc.yml --reporter mochawesome --reporter-options reportDir=report,reportFilename=test_results --timeout 30000",
    "test:one": "mocha --config ./src/.mocharc.yml --timeout 10000 --grep",
    "fix:tslint": "tslint --fix --project . -c tslint.json"
  },
  "author": "test",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.27",
    "@types/chai": "^4.2.1",
    "@types/chance": "^1.0.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "@types/node-fetch": "^2.5.0",
    "@types/request": "^2.48.2",
    "@types/request-promise-native": "^1.0.16",
    "@types/selenium-webdriver": "^4.0.5",
    "@types/uuid": "^3.4.6",
    "aws-sdk": "^2.510.0",
    "chai": "^4.2.0",
    "chance": "^1.0.18",
    "coveralls": "^3.0.4",
    "mocha": "^6.2.0",
    "mocha-typescript": "^1.1.17",
    "mochawesome": "^4.1.0",
    "nyc": "^14.0.0",
    "prettier": "^1.18.2",
    "request-promise-native": "^1.0.7",
    "rimraf": "^3.0.0",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-domain-manager": "^3.2.2",
    "serverless-offline": "^5.10.1",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-stack-output": "^0.2.3",
    "source-map-support": "^0.5.12",
    "ts-mockito": "^2.4.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "@types/lambda-log": "^2.2.0",
    "@types/traverson": "^2.0.28",
    "data-store": "^3.1.0",
    "lambda-log": "^2.3.1",
    "mysql": "^2.17.1",
    "selenium-webdriver": "3.6.0",
    "string-remove-thousand-separators": "^3.0.39",
    "traverson": "^6.1.1",
    "traverson-hal": "^6.1.0",
    "waiter": "^0.1.1",
    "uuid": "^3.3.3"
  }
}
